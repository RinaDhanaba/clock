RewriteEngine On

# Specific rule for "contact" page
RewriteRule ^contact$ /page/contact.php [L]

# General rule for clean URLs (applies to all other pages)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([^/]+)/?$ /page/$1.php [L]

# Custom error page
ErrorDocument 404 /404.php
